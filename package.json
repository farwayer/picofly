{
  "name": "picofly",
  "version": "0.0.17",
  "license": "MIT",
  "description": "Lightweight state manager, simple, fast and built with ‚ù§",
  "author": "farwayer <farwayer@gmail.com>",
  "repository": "https://github.com/farwayer/picofly",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.js",
    "./react": "./src/react/index.js",
    "./utils": "./src/utils/index.js",
    "./package.json": "./package.json"
  },
  "keywords": [
    "react",
    "react-native",
    "state",
    "state manager",
    "pico",
    "proxy",
    "store"
  ],
  "scripts": {
    "size": "size-limit",
    "test": "uvu",
    "publish": "clean-publish"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.2.0",
    "benchmark": "^2.1.4",
    "clean-publish": "^5.2.2",
    "microtime": "^3.1.1",
    "mobx": "^6.13.7",
    "picocolors": "^1.1.1",
    "react": "^19.1.1",
    "size-limit": "^11.2.0",
    "tinybench": "^5.0.1",
    "uvu": "^0.5.6",
    "valtio": "^2.1.7"
  },
  "workspaces": [
    "examples/*"
  ],
  "size-limit": [
    {
      "name": "Store obj minimal",
      "import": {
        "src/index.js": "{create, onWrite, obj}"
      },
      "limit": "515b"
    },
    {
      "name": "Store obj full",
      "import": {
        "src/index.js": "{create, obj, onWrite, onRead, lock, unlock}"
      },
      "limit": "533b"
    },
    {
      "name": "Store map full",
      "import": {
        "src/index.js": "{create, map, onWrite, onRead, lock, unlock}"
      },
      "limit": "904b"
    },
    {
      "name": "Store objIgnoreSpecials full",
      "import": {
        "src/index.js": "{create, objIgnoreSpecials, onWrite, onRead, lock, unlock}"
      },
      "limit": "624b"
    },
    {
      "name": "Store objMap full",
      "import": {
        "src/index.js": "{create, objMap, onWrite, onRead, lock, unlock}"
      },
      "limit": "1104b"
    },
    {
      "name": "Store objMapIgnoreSpecialsRef zero",
      "import": {
        "src/index.js": "{create, objMapIgnoreSpecialsRef}"
      },
      "limit": "1104b"
    },
    {
      "name": "React obj",
      "import": {
        "src/index.js": "{create, obj}",
        "src/react/index.js": "{StoreProvider, useStore}"
      },
      "ignore": [
        "react"
      ],
      "limit": "707b"
    },
    {
      "name": "React objIgnoreSpecials",
      "import": {
        "src/index.js": "{create, objIgnoreSpecials}",
        "src/react/index.js": "{StoreProvider, useStore}"
      },
      "ignore": [
        "react"
      ],
      "limit": "805b"
    },
    {
      "name": "React objMapSetIgnoreSpecialsRef",
      "import": {
        "src/index.js": "{create, objMapSetIgnoreSpecialsRef}",
        "src/react/index.js": "{StoreProvider, useStore}"
      },
      "ignore": [
        "react"
      ],
      "limit": "1598b"
    },
    {
      "name": "Utils",
      "import": {
        "src/utils/index.js": "{memo}"
      },
      "limit": "215b"
    }
  ],
  "clean-publish": {
    "fields": [
      "packageManager",
      "workspaces"
    ]
  },
  "packageManager": "yarn@4.9.4"
}
