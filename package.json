{
  "type": "module",
  "license": "WTFPL",
  "author": "farwayer <farwayer@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.js",
    "./react": "./src/react.js"
  },
  "scripts": {
    "size": "size-limit",
    "test": "uvu"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.0",
    "benchmark": "^2.1.4",
    "esbuild": "^0.19.5",
    "microtime": "^3.1.1",
    "mobx": "^6.10.0",
    "nanoid": "^5.0.3",
    "picocolors": "^1.0.0",
    "react": "^18.2.0",
    "size-limit": "^11.0.0",
    "tinybench": "^2.4.0",
    "uvu": "^0.5.6",
    "valtio": "^1.11.1"
  },
  "workspaces": [
    "examples/*"
  ],
  "size-limit": [
    {
      "name": "Store objIgnoreSpecials (default)",
      "path": "src/store.js",
      "import": "{store, onWrite, onRead, lock, unlock}",
      "limit": "1Kb"
    },
    {
      "name": "Store obj",
      "path": [
        "src/store.js",
        "src/proxify/index.js"
      ],
      "import": {
        "src/store.js": "{createStore, onWrite, onRead, lock, unlock}",
        "src/proxify/index.js": "{obj}"
      },
      "limit": "1Kb"
    },
    {
      "name": "Store objMap",
      "path": [
        "src/store.js",
        "src/proxify/index.js"
      ],
      "import": {
        "src/store.js": "{createStore, onWrite, onRead, lock, unlock}",
        "src/proxify/index.js": "{objMap}"
      },
      "limit": "1Kb"
    },
    {
      "name": "React",
      "path": [
        "src/store.js",
        "src/react.js"
      ],
      "import": {
        "src/store.js": "{store}",
        "src/react/use-store.js": "{StoreProvider, useStore}"
      },
      "ignore": [
        "react"
      ],
      "limit": "1Kb"
    }
  ],
  "packageManager": "yarn@4.0.2"
}
